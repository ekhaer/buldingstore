        <tr>
            <th>Product Name</th>
            <th>Price</th>
            <th>Qty</th>
            <th>Total Price</th>
            <th>Action</th>
        </tr>
        <% for(let i = 0; i < DataOrder.length; i++) { %>
        <form action="orderlist/<%= DataOrder[i].ProductId%>/delete" method="POST">
        <tr>
            <td><%= DataOrder[i].Product.prooductName %></td>
            <td><%= DataOrder[i].Product.price %></td>
            <td><input type="number" name="qty" value="<%= DataOrder[i].qty %>"></td>
            <td><%=  %></td>
            <td><a href="orderlist/<%= DataOrder[i].ProductId%>/delete">Delete</a></td>
        </tr>
        </form>
        <% } %>
    </table><br>
    <a href="/checkout">Download invoice</a>
</body>
</html>